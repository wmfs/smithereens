  },
  "bugs": {
    "url": "https://github.com/wmfs/smithereens/issues"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "csv-streamify": "4.0.0",
    "csv-string": "4.0.1",
    "debug": "4.2.0",
    "glob": "7.1.6",
    "hash-sum": "2.0.0",
    "jsonfile": "6.0.1",
    "make-dir": "3.1.0",
    "pump": "3.0.0",
    "upath": "1.2.0",
    "uuid": "7.0.3"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-subset": "1.6.0",
    "codecov": "3.8.0",
